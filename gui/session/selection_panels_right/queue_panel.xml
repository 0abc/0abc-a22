<?xml version="1.0" encoding="utf-8"?>
<object name="unitQueuePanel"
  size="-40 -48 100% 4"
  type="image"
  sprite="queuePanelShader"
>
  <object size="-4 -4 52 52" type="image" sprite="stretched:session/icons/production.png" tooltip_style="sessionToolTipBottom">
    <translatableAttribute id="tooltip">Production queue</translatableAttribute>
    <object name="queueTimeRemaining" ghost="true" style="iconButtonProgress" type="text"/>
  </object>

  <object size="44 4 100%-4 100%-4">
  <repeat count="60">
    <object name="unitQueueButton[n]" hidden="true" style="iconButton" type="button" size="0 0 36 36" tooltip_style="sessionToolTipBottom">
    <object name="unitQueueIcon[n]" ghost="true" type="image" size="2 2 34 34"/>
    <object name="unitQueueProgressSlider[n]" type="image" sprite="queueProgressSlider" ghost="true" size="2 2 34 34" z="20"/>
    <object name="unitQueueCount[n]" ghost="true" style="groupIconsText" type="text" z="20"/>
    </object>
  </repeat>
  </object>
</object>
