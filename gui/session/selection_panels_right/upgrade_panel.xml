<?xml version="1.0" encoding="utf-8"?>
<object name="unitUpgradePanel" size="4 4 100%-4 100%-4">
  <object size="0 0 100% 100%">
    <repeat count="20">
      <object name="unitUpgradeButton[n]" hidden="true" style="iconButton" type="button" size="0 0 36 36" tooltip_style="sessionToolTipBottom">
        <object name="unitUpgradeIcon[n]" type="image" ghost="true" size="2 2 34 34"/>
        <object name="unitUpgradeUpgradeIcon[n]" type="image" ghost="true" size="1 2 34 34" sprite="stretched:session/icons/upgrade.png"/>
        <object name="unitUpgradeProgressSlider[n]" type="image" sprite="queueProgressSlider" ghost="true" size="2 2 34 34" z="20"/>
        <object name="unitUpgradeSelection[n]" hidden="true" type="image" ghost="true" size="2 2 34 34" sprite="stretched:session/icons/corners.png"/>
        <object name="unitUpgradeCount[n]" ghost="true" style="groupIconsText" type="text" z="30"/>
      </object>
    </repeat>
  </object>
</object>
